    "url": "https://github.com/Madush99/Hotel-Management-MERN/issues"
  },
  "homepage": "https://github.com/Madush99/Hotel-Management-MERN#readme",
  "dependencies": {
    "aos": "^2.3.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.13.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "path": "^0.12.7",
